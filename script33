            for _, obj in ipairs(workspace:GetDescendants()) do
                if obj:IsA("BasePart") and isValidDangerName(obj.Name) then
                    foundCount = foundCount + 1
                    table.insert(blacklist, obj) -- Ø°Ø®ÛŒØ±Ù‡ Ø¯Ø± Ù„ÛŒØ³Øª Ø³ÛŒØ§Ù‡ Ø¨Ø±Ø§ÛŒ Ù‡Ù…ÛŒØ´Ù‡
