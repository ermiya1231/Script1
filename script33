local function cleanPhysics(obj, char)
    if not antiSlapEnabled then return end
    
    -- Ø­Ø°Ù Ù†ÛŒØ±ÙˆÙ‡Ø§ÛŒÛŒ Ú©Ù‡ Ø¨Ø§Ø¹Ø« Ù¾Ø±ØªØ§Ø¨ Ø´Ø¯Ù† ÛŒØ§ Ú†Ø±Ø®Ø´ Ú©Ø§Ø±Ø§Ú©ØªØ± Ù…ÛŒâ€ŒØ´ÙˆÙ†Ø¯
    if obj:IsA("BodyAngularVelocity") or obj:IsA("BodyGyro") or obj:IsA("BodyVelocity") then
        -- ÛŒÚ© Ú†Ú© Ø§Ù…Ù†ÛŒØªÛŒ Ø¨Ø±Ø§ÛŒ Ø§ÛŒÙ†Ú©Ù‡ Ù†ÛŒØ±ÙˆÙ‡Ø§ÛŒ Ø¶Ø±ÙˆØ±ÛŒ Ø®ÙˆØ¯ Ø¨Ø§Ø²ÛŒ Ø­Ø°Ù Ù†Ø´ÙˆÙ†Ø¯ (Ø·Ø¨Ù‚ Ø§Ø³Ú©Ø±ÛŒÙ¾Øª Ø§ØµÙ„ÛŒ Ø®ÙˆØ¯Øª)
        local isSafe = (obj:IsA("BodyGyro") and obj.P == 90000) or (obj:IsA("BodyVelocity") and obj.Velocity == Vector3.new(0,0,0))
        
        if not isSafe then
            pcall(function()
                obj:Destroy()
                if char and char:FindFirstChild("Humanoid") then
                    char.Humanoid.Sit = false
                    char.Humanoid.PlatformStand = false
                end
            end)
        end
    end
end

-- Ù…Ø§Ù†ÛŒØªÙˆØ± Ú©Ø±Ø¯Ù† Ú©Ø§Ø±Ø§Ú©ØªØ± Ø¨Ø±Ø§ÛŒ Ù†ÛŒØ±ÙˆÙ‡Ø§ÛŒ Ø¬Ø¯ÛŒØ¯
local function monitorCharacter(char)
    if not char then return end
    
    -- Ø¨Ø±Ø±Ø³ÛŒ Ø§Ø¬Ø²Ø§ÛŒ ÙØ¹Ù„ÛŒ
    for _, v in ipairs(char:GetDescendants()) do
        cleanPhysics(v, char)
    end
    
    -- Ø¨Ø±Ø±Ø³ÛŒ Ø§Ø¬Ø²Ø§ÛŒÛŒ Ú©Ù‡ Ø¨Ø¹Ø¯Ø§Ù‹ Ø§Ø¶Ø§ÙÙ‡ Ù…ÛŒâ€ŒØ´ÙˆÙ†Ø¯ (Ù…Ø«Ù„ ÙˆÙ‚ØªÛŒ Ú©Ø³ÛŒ Ø¨Ù‡ Ø´Ù…Ø§ Ø³ÛŒÙ„ÛŒ Ù…ÛŒâ€ŒØ²Ù†Ø¯)
    char.DescendantAdded:Connect(function(desc)
        if antiSlapEnabled then
            cleanPhysics(desc, char)
        end
    end)
end

