                if model and isnpc(model) and model ~= char then
                    local hrp = model:FindFirstChild("HumanoidRootPart")
                    local hum = model:FindFirstChildOfClass("Humanoid")
                    
                    if hrp and partowner(hrp) and not hrp.Anchored then
                        hum:ChangeState(Enum.HumanoidStateType.Dead)
                        hum.Health = 0
                    end
                end
            end
        end)
    else
        killBtn.Text = "ğŸ’€ NPC Kill Aura: OFF"
        killBtn.BackgroundColor3 = Color3.fromRGB(255, 90, 90)
        if killAuraConn then killAuraConn:Disconnect() end
    end
end)

