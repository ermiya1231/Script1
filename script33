local function toggleNoclip()
    if noclipEnabled then
        -- ÙØ¹Ø§Ù„ Ú©Ø±Ø¯Ù† NoClip Ø¨Ø§ Ø§Ø³ØªÙØ§Ø¯Ù‡ Ø§Ø² Ø­Ù„Ù‚Ù‡ ÙÛŒØ²ÛŒÚ©ÛŒ
        noclipConn = RunService.Stepped:Connect(function()
            if noclipEnabled and LocalPlayer.Character then
                for _, part in ipairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") and part.CanCollide then
                        part.CanCollide = false
                    end
                end
            end
        end)
        notify("NoClip Activated ğŸ‘»")
    else
        -- ğŸ›‘ Ø¨Ø®Ø´ Ø­ÛŒØ§ØªÛŒ: Ø®Ø§Ù…ÙˆØ´ Ú©Ø±Ø¯Ù† Ø¢Ù†ÛŒ
        if noclipConn then
            noclipConn:Disconnect()
            noclipConn = nil
        end
        
        -- Ø¨Ø±Ú¯Ø±Ø¯Ø§Ù†Ø¯Ù† ÙÙˆØ±ÛŒ Ø¨Ø±Ø®ÙˆØ±Ø¯ Ø¨Ù‡ ØªÙ…Ø§Ù… Ø§Ø¹Ø¶Ø§ÛŒ Ø¨Ø¯Ù†
        if LocalPlayer.Character then
            for _, part in ipairs(LocalPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
        notify("NoClip De-Activated âœ…")
    end
end

noclipBtn.MouseButton1Click:Connect(function()
    noclipEnabled = not noclipEnabled
    
    if noclipEnabled then
        noclipBtn.Text = "ğŸ‘» NoClip: ON"
        noclipBtn.BackgroundColor3 = Color3.fromRGB(80, 200, 120)
    else
        noclipBtn.Text = "ğŸ‘» NoClip: OFF"
        noclipBtn.BackgroundColor3 = Color3.fromRGB(255, 90, 90)
    end
    
    toggleNoclip()
end)

