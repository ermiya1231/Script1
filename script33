    if killAuraEnabled then
        killBtn.Text = "ğŸ’€ NPC Kill Aura: ON"
        killBtn.BackgroundColor3 = Color3.fromRGB(80, 200, 120)
        
        killAuraConn = RunService.Stepped:Connect(function()
            local char = LocalPlayer.Character
            local hrp1 = char and char:FindFirstChild("HumanoidRootPart")
            if not hrp1 then return end

            -- Ø§Ø³ØªÙØ§Ø¯Ù‡ Ø§Ø² SimulationRadius Ø¨Ø±Ø§ÛŒ Ø¨Ù‡ Ø¯Ø³Øª Ú¯Ø±ÙØªÙ† ÙÛŒØ²ÛŒÚ©
